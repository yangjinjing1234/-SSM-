<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bdqn.dao.RoleDao">
   
   <select id="findAll" resultType="Role">
   		SELECT 	id, 
		roleCode, 
		roleName, 
		createdBy, 
		creationDate, 
		modifyBy, 
		modifyDate
		FROM 
		smbms_role 
